{
  "name": "express-es6-sample",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "NODE_ENV=development npm run watch ./src/bin/www",
    "watch": "nodemon --config ./nodemon.json --",
    "build": "npm run compile && NODE_ENV=production node ./dist/bin/www",
    "compile": "npm run clean && babel ./src --out-dir dist",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "morgan": "~1.9.0",
    "rimraf": "^2.6.3",
    "start": "babel-node ./src/bin/www"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "eslint": "^5.13.0",
    "eslint-plugin-import": "^2.16.0",
    "nodemon": "^1.18.9"
  }
}
